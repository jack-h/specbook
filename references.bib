	Journal = {American Astronomical Society Meeting 207},
	Month = {Dec},
	Pages = {1488},
	Pmid = {2005AAS...20719203G},
	Rating = {0},
	Title = {ALFALFA: a Legacy Extragalactic HI Survey},
